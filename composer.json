{
	"name": "code-orange/jot",
	"description": "Laravel package that generates RESTful API documentation in Markdown based on PHPDoc.",
	"keywords": [
		"laravel",
		"phpdoc",
		"documentation"
	],
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "Tim van Dalen",
			"email": "tim@code-orange.nl"
		}
	],
	"require": {
		"php": ">=5.6.4",
		"illuminate/console": "~7.0|~8.0",
		"illuminate/support": "~7.0|~8.0",
		"illuminate/routing": "~7.0|~8.0",
		"illuminate/view": "~7.0|~8.0",
		"phpdocumentor/reflection-docblock": "2.0.*"
	},
	"autoload": {
		"psr-4": {
			"CodeOrange\\Jot\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"CodeOrange\\Jot\\JotServiceProvider"
			]
		}
	}
}
